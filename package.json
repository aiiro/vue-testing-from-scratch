{
  "name": "vue-testing-from-scratch",
  "version": "1.0.0",
  "description": "Testing vue from scratch",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/jest-vue-preprocessor"
    }
  },
  "author": "aiiro <aiiro4github@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "vue": "^2.5.2"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^21.2.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "jest": "^21.2.1",
    "jest-vue-preprocessor": "^1.2.0",
    "vue-loader": "^13.3.0",
    "vue-template-compiler": "^2.5.2",
    "vue-test-utils": "^1.0.0-beta.2"
  }
}
